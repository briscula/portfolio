{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install && pnpm --filter @repo/database build && cd apps/api && pnpm build",
  "installCommand": "cd ../.. && pnpm install",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "OPTIONS", "DELETE", "HEAD"]
    }
  ]
}