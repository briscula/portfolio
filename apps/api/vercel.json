{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install && pnpm turbo build --filter=@repo/api... && cd apps/api && node scripts/copy-workspace-deps.js",
  "installCommand": "cd ../.. && pnpm install",
  "outputDirectory": "apps/api",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS", "HEAD"]
    }
  ]
}