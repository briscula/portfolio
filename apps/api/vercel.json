{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install && pnpm turbo build --filter=@repo/api...",
  "installCommand": "cd ../.. && pnpm install",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "OPTIONS", "DELETE", "HEAD"]
    }
  ]
}